<?xml version="1.0" encoding="UTF-8"?>
<skin version="0.2">
	<title xml:lang="ko">출첵 도장 스킨 Mh</title>
	<title xml:lang="en">Attendance Stamp Skin Mh</title>
	<description xml:lang="ko">
		출석부 출첵 도장 스킨
		제작: prologos(http://a3ghost.dyndns.org)
	</description>
	<version>1.0</version>
	<date>2017-07-25</date>
	<author email_address="prologos.35@gmail.com" link="http://a3ghost.dyndns.org">
		<name xml:lang="ko">prologos</name>
		<name xml:lang="en">prologos</name>
	</author>

	<license>Free</license>

	<extra_vars>
		<var name="title_sm" type="image">
			<title xml:lang="ko">상단 이미지</title>
			<description xml:lang="ko">작성하면 화면에 표시 됨</description>
		</var>

		<var name="board_type" type="select">
			<title xml:lang="ko">출석 스킨 스타일 설정</title>
			<options value="board">
				<title xml:lang="ko">Board 게시판형 (기본값)</title>
			</options>
			<options value="twitter">
				<title xml:lang="ko">Twitter (트위터 스타일)</title>
			</options>
			<description />
		</var>

		<var name="display_setup_button" type="select">
			<title xml:lang="ko">설정 표시</title>
			<options value="Y">
				<title xml:lang="ko">출력</title>
			</options>
			<options value="N">
				<title xml:lang="ko">출력하지 않음</title>
			</options>
			<description />
		</var>

		<var name="greetings_cut_size" type="text">
			<title xml:lang="ko">출석인사말갯수</title>
		</var>

		<var name="display_ip_address" type="select">
			<title xml:lang="ko">아이피주소 표시</title>
			<title xml:lang="en">Display ip address</title>
			<title xml:lang="jp">IPアドレス表示</title>
			<title xml:lang="zh-CN">显示IP地址</title>
			<title xml:lang="es-CN">Display ip address</title>
			<title xml:lang="zh-TW">顯示IP位址</title>
			<options value="Y">
				<title xml:lang="ko">출력</title>
				<title xml:lang="en">display</title>
				<title xml:lang="jp">表示</title>
				<title xml:lang="zh-CN">显示</title>
				<title xml:lang="es">display</title>
				<title xml:lang="zh-TW">顯示</title>
			</options>
			<options value="N">
				<title xml:lang="ko">출력하지 않음</title>
				<title xml:lang="en">not display</title>
				<title xml:lang="jp">非表示</title>
				<title xml:lang="zh-CN">不显示</title>
				<title xml:lang="es">not display</title>
				<title xml:lang="zh-TW">隱藏</title>
			</options>
		</var>

		<var name="pomis" type="text">
			<title xml:lang="ko">출석권한자</title>
			<description xml:lang="ko">출석가능한 회원그룹또는 설정값을 적으세요.</description>
		</var>

		<var name="timestart" type="text">
			<title xml:lang="ko">출석 가능 시작 시간</title>
			<description xml:lang="ko">출석 가능 시간을 표기합니다. 이란은 출책 시작시간입니다. (ex, 01:00:00 (시, 분, 초) 기본값 : 0시 0분 0초 )</description>
		</var>

		<var name="timeend" type="text">
			<title xml:lang="ko">출석 가능 마감 시간</title>
			<description xml:lang="ko">출석 가능 시간을 표기합니다. 이란은 출책 마감시간입니다. (ex, 01:00:00 (시, 분, 초) 기본값 : 24시 0분 0초 )</description>
		</var>
		<var name="no_profile" type="image">
			<title xml:lang="ko">프로필 이미지</title>
			<description xml:lang="ko">프로필 이미지가 없는 사람들에게 띄울 프로필 이미지입니다. 이는 Twitter Style에서만 설정됩니다.</description>
		</var>

	</extra_vars>
</skin>
